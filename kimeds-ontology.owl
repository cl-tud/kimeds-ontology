                                      owl:versionInfo "1.0.0" ;
                                      rdfs:label "The KIMEDS Ontology"@en ;
                                      rdfs:seeAlso <https://cl-tud.github.io/kimeds-ontology/> ;
                                      <http://purl.org/dc/terms/modified> "2023-10-23"^^xsd:date .


