                                                                     owl:versionIRI <http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology1.0.0> ; 
                                      <http://purl.org/dc/elements/1.1/description> "The KIMEDS Ontology defines various notions related to risk management for medical devices."@en ;
                                      <http://purl.org/dc/elements/1.1/title> "The KIMEDS Ontology"@en ; 
                                      <http://purl.org/dc/terms/created> "2023-05-02"^^xsd:date ;
