                                                                     <http://purl.org/dc/terms/created> "2023-09-12"^^xsd:date ;
                                                                     <http://purl.org/dc/terms/title> "Digital Risk Management Ontology"@en ;
                                                                     rdfs:label "The KIMEDS Ontology"@en ;
                                                                     rdfs:seeAlso <https://cl-tud.github.io/kimeds-ontology/> ;
