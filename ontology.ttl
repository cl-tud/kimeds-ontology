[ owl:intersectionOf ( :AssuranceSDA
                       :SDAI
                     ) ;
  rdf:type owl:Class ;
  rdfs:subClassOf :AssuranceSDAI
] .


[ owl:intersectionOf ( :RiskSDA
                       :SDAI
                     ) ;
  rdf:type owl:Class ;
  rdfs:subClassOf :RiskSDAI
] .


[ owl:intersectionOf ( :SDA
