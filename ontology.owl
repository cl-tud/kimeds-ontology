        <rdfs:range rdf:resource="https://w3id.org/riskman/ontology#HazardousSituation"/>
        <rdfs:label>has parent situation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/riskman/ontology#hasPrecedingEvent -->


