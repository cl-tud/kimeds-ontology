        <rdfs:range rdf:resource="https://w3id.org/riskman/ontology#Probability"/>
        <rdfs:comment>Probability of a hazardous situation leading to harm.</rdfs:comment>
        <rdfs:label>has Probability2</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/riskman/ontology#hasResidualRiskLevel -->


    <owl:ObjectProperty rdf:about="https://w3id.org/riskman/ontology#hasResidualRiskLevel">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/riskman/ontology#hasRiskLevel"/>
        <rdfs:domain rdf:resource="https://w3id.org/riskman/ontology#ControlledRisk"/>
        <rdfs:range rdf:resource="https://w3id.org/riskman/ontology#RiskLevel"/>
