    <!-- http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#AcceptanceMatrix -->


    <owl:Class rdf:about="http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#AcceptanceMatrix">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#RiskMatrix"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#acceptance"/>
                        <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>A tuple containing the combination of severity, probability and information whether such combination is acceptable or not.</rdfs:comment>
        <rdfs:label>Acceptance matrix</rdfs:label>
    </owl:Class>
    


    <!-- http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#AssuranceSDA -->


    <owl:Class rdf:about="http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#AssuranceSDA">
        <rdfs:subClassOf rdf:resource="http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#SDA"/>
        <owl:disjointWith rdf:resource="http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#RiskSDA"/>
        <rdfs:comment>A safe design argument (SDA), where the purpose is to make a safety assurance</rdfs:comment>
        <rdfs:label>Assurance SDA</rdfs:label>
    </owl:Class>
    


    <!-- http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#AssuranceSDAI -->


    <owl:Class rdf:about="http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#AssuranceSDAI">
        <rdfs:subClassOf rdf:resource="http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#AssuranceSDA"/>
        <rdfs:comment>A safe design argument (SDA), where the purpose is to make a safety assurance</rdfs:comment>
        <rdfs:label>Assurance SDAI</rdfs:label>
    </owl:Class>
    


    <!-- http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#DeviceComponent -->


    <owl:Class rdf:about="http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#DeviceComponent">
        <rdfs:comment>A (physical or logical) part of a device.</rdfs:comment>
        <rdfs:label>Device component</rdfs:label>
    </owl:Class>
    


    <!-- http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#DeviceContext -->


    <owl:Class rdf:about="http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#DeviceContext">
        <rdfs:comment>Information about possible environments the device can be used in that may affect probability or severity of some risk.</rdfs:comment>
        <rdfs:label>Device context</rdfs:label>
    </owl:Class>
    


    <!-- http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#DeviceSpecificHazard -->


    <owl:Class rdf:about="http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#DeviceSpecificHazard">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#hasDeviceComponent"/>
                        <owl:someValuesFrom rdf:resource="http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#DeviceComponent"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#hasFunction"/>
