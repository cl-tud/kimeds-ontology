    <!-- https://w3id.org/riskman/ontology#probability -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/riskman/ontology#probability">
        <rdfs:domain rdf:resource="https://w3id.org/riskman/ontology#RiskLevel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Probability of occurrence of harm, expressed as an integer from 1 to MAX (MAX being defined by one of the RiskTable dimensions), with 1 as the least probable and MAX as the most. The probability of occurrence of harm (P) can additionally be expressed as a combination of separate probabilities (P = P1*P2). A decomposition into P1 and P2 is not mandatory.</rdfs:comment>
        <rdfs:label>probability</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/riskman/ontology#probabilityMax -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/riskman/ontology#probabilityMax">
        <rdfs:domain rdf:resource="https://w3id.org/riskman/ontology#RiskMatrix"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>The maximal value representing the most probable risks.</rdfs:comment>
        <rdfs:label>probability max</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/riskman/ontology#severity -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/riskman/ontology#severity">
