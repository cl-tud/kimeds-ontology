    <owl:ObjectProperty rdf:about="https://w3id.org/riskman/ontology#hasHarm">
        <rdfs:range rdf:resource="https://w3id.org/riskman/ontology#Harm"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/riskman/ontology#hasAnalyzedRisk"/>
            <rdf:Description rdf:about="https://w3id.org/riskman/ontology#hasHarm"/>
        </owl:propertyChainAxiom>
        <rdfs:label>has harm</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/riskman/ontology#hasHazard -->


    <owl:ObjectProperty rdf:about="https://w3id.org/riskman/ontology#hasHazard">
