                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#hasHazardousSituation"/>
                        <owl:someValuesFrom rdf:resource="http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#HazardousSituation"/>
                    </owl:Restriction>
                </owl:intersectionOf>
