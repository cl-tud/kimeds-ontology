    <!-- https://w3id.org/riskman/ontology#hasRiskLevel -->


    <owl:ObjectProperty rdf:about="https://w3id.org/riskman/ontology#hasRiskLevel">
        <rdfs:range rdf:resource="https://w3id.org/riskman/ontology#RiskLevel"/>
        <rdfs:label>has risk level</rdfs:label>
    </owl:ObjectProperty>
    


