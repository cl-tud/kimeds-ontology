        <rdfs:label>Function</rdfs:label>
    </owl:Class>
    


    <!-- http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#Harm -->


    <owl:Class rdf:about="http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#Harm">
        <rdfs:comment>Injury or damage to the health of people, or damage to property or the environment.</rdfs:comment>
        <rdfs:label>Harm</rdfs:label>
    </owl:Class>
    


    <!-- http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#Hazard -->


