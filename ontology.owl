        <rdfs:label>Event</rdfs:label>
    </owl:Class>
    


    <!-- http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#Function -->


    <owl:Class rdf:about="http://semanticweb.org/kettmann/ontologies/2023/5/kimeds-ontology#Function">
